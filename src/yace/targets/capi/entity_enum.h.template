{% if entity.doc %}
/**
 * {{ entity.doc.brief }}
 *
{%- if entity.doc.description %}
 * {{ entity.doc.description }}
 *
{%- endif %}
 * @enum {{ entity.sym }}
 */
{%- endif %}
enum {{ entity.sym }} {
{%- for member in entity.members %}
	{{ member.sym }} = {{ '0x%0x' % member.val.lit }},{% if member.doc %} ///< {{ member.doc.brief }}{% endif %}
{%- endfor %}
};
